{
    "name": "giacomo/laravel-omnipay",
    "description": "Omnipay Service Provider for Laravel",
    "keywords": ["laravel", "omnipay"],
    "license": "MIT",
    "authors": [
        {
            "name": "Barry vd. Heuvel",
            "email": "barryvdh@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "5.0.* || 5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.*",
        "php-http/guzzle6-adapter": "^1.1",
        "omnipay/common": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Barryvdh\\Omnipay\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        },
        "laravel": {
            "providers": [
                "Barryvdh\\Omnipay\\ServiceProvider"
            ],
            "aliases": {
                "Omnipay": "Barryvdh\\Omnipay\\Facade"
            }
        }
    },
    "minimum-stability": "dev"
}
